{
    "manifest_version": 3,
    "name": "Tab Organizer",
    "version": "1.0",
    "permissions": ["tabs", "activeTab", "storage", "webNavigation"],
    "icons": {
      "48": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon.png"
      }
    }
  }